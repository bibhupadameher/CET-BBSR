<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TabHost
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/tabHost"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TabWidget
                android:id="@android:id/tabs"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="#9AAA9D"></TabWidget>

            <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:id="@+id/tab1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:background="@drawable/iam">

                    <WebView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/webView2"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:visibility="gone" />

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/imageView"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true" />

                    <ProgressBar
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:id="@+id/progressBar3"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="117dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/tab2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:background="@drawable/iam">

                    <ScrollView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:id="@+id/scrollView"
                        >
                        <RelativeLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent">


                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton"
                                android:layout_marginLeft="33dp"
                                android:layout_marginStart="33dp"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_marginTop="60dp"
                                android:background="@drawable/qa"/>

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton2"
                                android:background="@drawable/qb"
                                android:layout_alignTop="@+id/imageButton"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentEnd="true"
                                android:layout_marginRight="33dp"
                                android:layout_marginEnd="33dp" />
                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton88"
                                android:layout_marginTop="50dp"
                                android:background="@drawable/iooo"
                                android:layout_below="@+id/imageButton"
                                android:layout_alignLeft="@+id/imageButton"
                                android:layout_alignStart="@+id/imageButton" />


                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton3"
                                android:layout_marginTop="50dp"
                                android:background="@drawable/qcc"
                                android:layout_below="@+id/imageButton88"
                                android:layout_alignLeft="@+id/imageButton"
                                android:layout_alignStart="@+id/imageButton" />
                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton87"
                                android:layout_alignTop="@+id/imageButton88"
                                android:layout_alignLeft="@+id/imageButton2"
                                android:layout_alignStart="@+id/imageButton2"
                                android:background="@drawable/iio"/>

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton10"
                                android:layout_above="@+id/imageButton11"
                                android:layout_alignLeft="@+id/imageButton6"
                                android:layout_alignStart="@+id/imageButton6"
                                android:background="@drawable/qf" />

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton6"
                                android:layout_alignTop="@+id/imageButton3"
                                android:layout_alignLeft="@+id/imageButton2"
                                android:layout_alignStart="@+id/imageButton2"
                                android:background="@drawable/qd"/>

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton9"
                                android:background="@drawable/qe"
                                android:layout_below="@+id/imageButton3"
                                android:layout_marginTop="50dp"
                                android:layout_alignLeft="@+id/imageButton3"
                                android:layout_alignStart="@+id/imageButton3" />

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton12"
                                android:background="@drawable/qh"
                                android:layout_alignParentBottom="true"
                                android:layout_alignLeft="@+id/imageButton10"
                                android:layout_alignStart="@+id/imageButton10" />

                            <ImageButton
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:id="@+id/imageButton11"
                                android:background="@drawable/qg"
                                android:layout_below="@+id/imageButton9"
                                android:layout_alignLeft="@+id/imageButton9"
                                android:layout_alignStart="@+id/imageButton9"
                                android:layout_marginTop="50dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceLarge"
                                android:text="CET BBSR"
                                android:id="@+id/textView5"
                                android:textStyle="bold"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:textColor="#7a1a1a" />

                        </RelativeLayout>

                    </ScrollView>


                </RelativeLayout>

            </FrameLayout>
        </LinearLayout>
    </TabHost>
</RelativeLayout>